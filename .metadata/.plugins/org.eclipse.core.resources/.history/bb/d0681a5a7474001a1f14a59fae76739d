package package_aluno;

import javax.swing.JOptionPane;


public class Main {
	public static void main (String[] args) {
		String opc = "c";
		
		for (int c = 1; c <=3 ; c++) {
			GestaoAlunos.criar();
		}
		
		JOptionPane.showMessageDialog(null, "Terminou");
		
		switch(opc) {
			case "c": GestaoAlunos.criar();
			break;
			case "e": GestaoAlunos.exibir(aluno);
			break;
			case "a": GestaoAlunos.atualizar();
			break;
			case "r": GestaoAlunos.excluir();;
			break;
			case "s": System.exit(0);
		}
	
	}
}
